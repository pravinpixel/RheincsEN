    <style>
        body, html {
            font-family: Raleway !important;
        }

        ul.social_share_privacy_area li .info {
            display: none !important;
        }

        body {
            opacity: 0;
        }

		.bx-wrapper{ border:none !important; box-shadow: none !important;
		}
        .gitex-banner-container {
            position: fixed;
            background: rgba(0, 0, 0, 0.6);
            top: 0px;
            left: 0px;
            z-index: 9999;
            width: 100%;
            height: 100%;
        }

        .gitex-banner-img {
            position: relative;
            top: 30%;
            left: 0%;
        }

        .gitex-banner-img-shadow {
            border: 2px solid #999; /*box-shadow: 1px 1px 1px 1px #b72695;*/
        }

        .gitex-img {
            cursor: pointer;
        }

        .close-button-position {
            position: absolute;
            background: #b72695;
            border-radius: 12px;
            height: 28px;
            width: 26px;
            padding: 3px;
            top: -4%;
            left: 71.3%;
            cursor: pointer;
        }

        .close-icon-white {
            color: #fff;
            padding: 6px;
        }

        @media (min-width: 348px) and (max-width: 620px) {
            .close-button-position {
                left: 74.3%;
            }
        }

        @media (min-width:621px) and (max-width: 640px) {
            .close-button-position {
                left: 68.3%;
            }
        }

        @media (min-width:641px) and (max-width: 768px) {
            .close-button-position {
                left: 88.3%;
            }
        }

        @media (min-width:769px) and (max-width: 1024px) {
            .close-button-position {
                left: 78.3%;
            }
        }

        @media only screen and (max-width: 335px) {
            .close-button-position {
                left: 86.3%;
            }
        }

        .custom-form.modal-dialog button.close {
            font-size: 16px;
            color: #fff;
            background: #EC1D23;
            opacity: 1;
            border-radius: 50%;
            height: 30px;
            width: 30px;
            line-height: 30px;
            z-index: 99;
            position: absolute;
            top: -10px;
            right: -25px;
        }

        .modal-dialog.custom-form.pop-ups {
            width: 480px;
            margin: 30px auto;
        }

        @media (max-width: 768px) {
            .modal-dialog.custom-form.pop-ups {
                width: 92%;
                margin: 30px auto;
            }
        }
    </style>

